    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@emotion/styled": "^10.0.11",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-react-displayname": "0.0.5",
